<li class="model story <%= mark_if_closed(issue) %>" id="<%= story_html_id_or_empty(issue) %>">
  <div class="id story_tooltip story_field">
    <div class="tooltip_text"><%= render :partial => "rb_stories/tooltip", :object => issue %></div>
    <div class="t"><%= issue_link_or_empty(issue) %></div>
    <div class="v"><%= id_or_empty(issue) %></div>
  </div>
  <div class="tracker_id story_field" fieldtype="select" fieldname="tracker_id">
    <div class="t"><%= tracker_name_or_empty(issue) %></div>
    <div class="v"><%= tracker_id_or_empty(issue) %></div>
  </div>
  <div class="subject editable story_field" fieldname="subject" fieldtype="textarea"><%=h issue.subject %></div>
  <div class="status_id editable story_field" fieldtype="select" fieldname="status_id">
    <div class="t"><%= status_label_or_default(issue) %></div>
    <div class="v"><%= status_id_or_default(issue) %></div>
  </div>
  <div class="story_points" fieldname="story_points"><%= story_points_or_empty(issue) %></div>
  <div class="fixed_version_id"><%= issue.fixed_version_id %></div>
  <div class="higher_item_id"><%= issue.higher_item.blank? ? '' : issue.higher_item.id %></div>
  <div class="meta">
    <%= render :partial => "backlogs/model_errors", :object => issue.errors %>
  </div>
  <div class="clearfix"></div>
</li>
